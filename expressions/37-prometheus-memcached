sum (memcached_commands_total{instance=~"$node", status="miss"})  / sum (memcached_commands_total{instance=~"$node"}) 
sum (memcached_current_connections{instance=~"$node"}) by (instance)
sum (memcached_commands_total{instance=~"$node", command="set"}) / sum (memcached_commands_total{instance=~"$node", command="get"})
sum (memcached_commands_total{instance=~"$node"}) by (command)
sum(memcached_items_evicted_total{instance=~"$node"})
sum(memcached_items_reclaimed_total{instance=~"$node"})
sum(irate(memcached_read_bytes_total{instance=~"$node"}[5m]))
irate(memcached_written_bytes_total{instance=~"$node"}[10m])
sum(memcached_current_bytes{instance=~"$node"}) / sum(memcached_limit_bytes{instance=~"$node"})
sum (memcached_current_items{instance=~"$node"})
