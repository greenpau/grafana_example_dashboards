irate(haproxy_frontend_requests_denied_total{frontend=~"$frontend", alias="$alias"}[180s])
irate(haproxy_frontend_bytes_in_total{frontend=~"$frontend", alias="$alias"}[180s])
irate(haproxy_frontend_bytes_out_total{frontend=~"$frontend", alias="$alias"}[180s])
irate(haproxy_frontend_connections_total{frontend=~"$frontend", alias="$alias"}[180s])
